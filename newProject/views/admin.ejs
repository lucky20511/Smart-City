<html ng-app="adminApp">
<head>
  <meta charset="utf-8">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
  <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/highcharts-3d.js"></script>
    <script src="https://code.highcharts.com/modules/exporting.js"></script>
    <script  src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.js"></script>
    <script src="javascripts/admin-operations.js"> </script>
    <script src="http://d3js.org/d3.v3.min.js"></script>
    <script src="http://labratrevenge.com/d3-tip/javascripts/d3.tip.v0.6.3.js"></script>
    <link type="text/css" rel="stylesheet" href="/stylesheets/style.css"> </link>
    <script src="/javascripts/ForceGraph.js" > </script>
    <link href="/javascripts/vis/dist/vis.css" rel="stylesheet" type="text/css" />
    <script src="javascripts/vis/dist/vis.js"></script>
</head>
<body ng-controller="adminCtrl">

<div class="container-fluid">
    <!--<div class="top bar">-->
        <!--<div class="col-sm-12">-->
        <!--This is the top bar-->
        <!--</div>-->
    <!--</div>-->
    <div class="row content">
        <br>
        <div class="col-md-2 sidenav">
            <h4><strong>Admin Panel</strong></h4>
            <ul class="nav nav-pills nav-stacked">
                <li class="active"><a href="/profile">Profile</a></li>
                <li><a href="/moderator">Moderator</a></li>
                <li><a href="/login">Login</a></li>
                <li><a href="#section3">Photos</a></li>
            </ul><br>

        </div>
        <div class="col-md-4">
            <hr>
            <h2>List of Clusters</h2>
            <h5><span class="glyphicon glyphicon-time"></span> as of May 14, 2017.</h5>
            <table >
                <tr>
                    <th>Cluster </th>
                    <th>&nbsp;&nbsp;&nbsp;&nbsp;Name of User </th>
                </tr>
                <tr ng-repeat="x in adminFile | orderBy: 'clustername' ">
                    <td>{{x.clustername}}
                    </td>
                    <td>{{x.names}}</td>
                    <td>
                        <!-- Button to delete the clusters-->
                        &nbsp; &nbsp;&nbsp;&nbsp;<a href="#" class="btn btn-info btn-sm" ng-click="delete($index)">
                            <span class="glyphicon glyphicon-trash"></span>Delete
                        </a>
                    </td>
                </tr>
            </table>
        </div>

        <div class="col-md-6 ">
            <div class="dash-unit">
                <hr>
                <div class="section-graph">
                    <div id="importantchart"></div>
                    <br>
                    <div id="container" style="height: 400px">

                </div>
                </div>
            </div>
        </div>

    <!--<div class="row">-->
        <div class="col-md-12 forcechart">
                <hr>
                <div class="force-graph">
                    <div id="forceChart"></div>
                    <br>
                    <div id="forceContainer" style="height: 400px">

                    </div>
                </div>
        </div>
        </div>
    <!--</div>-->

</div>





<footer class="container-fluid">
    <p>Smart City</p>
</footer>


</body>
</html>